# .gitignore for Grails 1.2 and 1.3

# web application files
/web-app/plugins

# IDE support files
/.classpath
/.launch
/.project
/.settings
/*.launch
/*.tmproj
/ivy*
/eclipse
/.idea/
*.iml

# default HSQL database files for production mode
/prodDb.*

# general HSQL database files
*Db.properties
*Db.script
/*DB.*

# logs
/stacktrace.log
/test/reports
/logs
/*.log

# project release file
/*.war

# plugin release file
/*.zip

# older plugin install locations
/plugins
/web-app/plugins
/web-app/WEB-INF/classes
/plugin.xml
/plugin/

# "temporary" build files
/target
/out/

# other
*.iws
/cobertura.ser
.DS_Store